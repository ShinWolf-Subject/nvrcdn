{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MAX_FILE_SIZE": "134217728",
    "FILE_LIFETIME_HOURS": "5",
    "BAN_DURATION_HOURS": "3"
  }
}
